{
	"compilerOptions": {
		"sourceMap": false,
		"outDir": "../../../dist/out-tsc",
		"allowJs": true,
		"types": ["cypress", "node"],
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"cypress/**/*.ts",
		"cypress/**/**/*.ts",
		"cypress/**/**/**/*.ts",
		"cypress/**/*.js"
	]
}
